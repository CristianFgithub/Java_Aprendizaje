package es.cristian.poo;

public class Vehiculo {

	public Vehiculo() { //constructor de la clase. mismo nombre que la clase.
		
		ruedas=4;
		
		largo=2;
		
		ancho=1;
		
		peso=2;
		
		color="Sin color";
		
		
	}

	public void setColor() { //método setter establece valor de propiedad
		
		color="azul";
		
	}
	
	public String getColor(){ //método getter devuelve valor de propiedad
		
		
		
		return color;
		
	}
	
	
	private int ruedas;
	
	private String color;
	
	private int largo;
	
	private int ancho;
	
	private int peso;
}
